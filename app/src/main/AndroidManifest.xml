<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.moksleivis.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_names"
        android:logo="@drawable/people"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".UserAccount"
            android:label="@string/title_activity_user_account"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".vartotojas"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_vartotojas"
            android:theme="@style/FullscreenTheme"></activity>
    </application>

</manifest>
